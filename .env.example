# Kubernetes Configuration (optional)
KUBECONFIG=~/.kube/config
DEBUG_LEVEL=1

# AI Configuration (Google Gemini)
AI_GOOGLE_API_KEY=your-google-api-key-here
AI_MODEL=gemini-2.0-flash
AI_TEMPERATURE=0.7

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379/0

# Fortune Tools Configuration
# 設定為 true 啟用算命工具，設定為 false 關閉（預設：false）
ENABLE_FORTUNE_TOOLS=false

# Fortune Telling API Keys (optional, 僅在 ENABLE_FORTUNE_TOOLS=true 時需要)
YUANFENJU_API_KEY=your-yuanfenju-api-key-here
SERPAPI_API_KEY=your-serpapi-api-key-here

# LINE Bot Configuration (optional)
LINE_CHANNEL_SECRET=your_channel_secret_here
LINE_CHANNEL_ACCESS_TOKEN=your_access_token_here

# Application Configuration
LOG_LEVEL=INFO